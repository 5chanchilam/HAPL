      "source": [
        "def is_leap_year(year):\n",
        "    if year < 1582:\n",
        "        return year % 4 == 0\n",
        "    else:\n",
        "        return year % 4 == 0 and (year % 100 != 0 or year % 400 == 0)\n",
        "\n",
        "year = int(input(\"請輸入年份: \"))\n",
        "\n",
        "if is_leap_year(year):\n",
        "    print(year, \"年是閏年\")\n",
        "else:\n",
        "    print(year, \"年是平年\")"
      ],
